type Menu{
    id:String!,
    time:String,
    list:[String],
    name:String,
    typeInt:Int
}

input MenuInput{
    id:String
}

input MenuListInput{
    pageNo:Int!,
    pageSize:Int!
    name:String,
    typeInt:Int,
    time:String
}


type Query{
    menu(input:MenuInput):Menu!
    menuList(input:MenuListInput):[Menu]!
}

